{
  "compilerOptions": {
    "module": "nodenext",
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "moduleResolution": "nodenext",
    "outDir": "../out/integ/spec",
    "sourceMap": true,
    "strict": true,
    "rootDir": "spec",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "spec",
    ".eslintrc.json"
  ],
  "exclude": [
    "node_modules",
    "resources/**"
  ]
}
